{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"],
      "vue-color/dist/types/composable/colorModel": ["./types/vue-color.d.ts"]
    },
    "types": ["./node_modules/vue-color/dist/types/index.d.ts", "./types/vue-color.d.ts"]
  }
}
