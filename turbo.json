{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "cache/**"],
      "env": ["TURBO_REMOTE_CACHE_SIGNATURE_KEY"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "esl:dev": {
      "cache": false,
      "persistent": true
    },
    "esl:build": {}
  },
  "remoteCache": {
    "signature": true
  }
}
